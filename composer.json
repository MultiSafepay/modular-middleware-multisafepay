{
    "name": "modular-middleware/modular-middleware-multisafepay",
    "description": "This is my package modular-middleware-multisafepay",
    "keywords": [
        "Modular-Multisafepay",
        "laravel",
        "modular-middleware-multisafepay"
    ],
    "homepage": "https://github.com/modular-multisafepay/modular-middleware-multisafepay",
    "license": "MIT",
    "authors": [
        {
            "name": "MultiSafepay",
            "email": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Modular-Multisafepay\\ModularMiddlewareMultisafepay\\": "src",
            "Modular-Multisafepay\\ModularMiddlewareMultisafepay\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modular-Multisafepay\\ModularMiddlewareMultisafepay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modular-Multisafepay\\ModularMiddlewareMultisafepay\\ModularMiddlewareMultisafepayServiceProvider"
            ],
            "aliases": {
                "ModularMiddlewareMultisafepay": "Modular-Multisafepay\\ModularMiddlewareMultisafepay\\Facades\\ModularMiddlewareMultisafepay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
